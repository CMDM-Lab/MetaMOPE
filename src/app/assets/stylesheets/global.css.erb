.msgBox{
        position:absolute;
	top: 20px;
        box-shadow: 0 2px 10px 0 rgba(0, 0, 0, 0.16), 0 2px 5px 0 rgba(0, 0, 0, 0.4);
        z-index:1031;
        width: 320px;
        left:100%;
        font-size:14px;
        padding:14px;
}
.errorMsgBg{
        color:rgb(128, 21, 21);
        background-color:rgb(242, 222, 222);
}
.warnMsgBg{
        color:rgb(16, 94, 42);
        background-color:rgb(177, 235, 174);
}
.clickClose{
        position:absolute;
        right:5px;
        top:0px;
        display:none;
}
